{
    "/tags": {
        "headers": {
            "Accept": "application/json"
        },
        "post": {
            "201": {
              "body": {
                "body": {
                  "name": "created-tag-name",
                  "description": "created-tag-desc",
                  "id": "created-tag-id"
                }
              }
            }
        }
    },
    "/tags/{id}": {
        "headers": {
            "Accept": "application/json"
        },
        "get": {
            "200": {
                "path": {
                    "id": "get-tag"
                }
            },
            "404": {
                "path": {
                    "id": "not-found-tag"
                }
            }
        },
        "put": {
            "200": {
                "path": {
                    "id": "put-tag"
                }
            },
            "404": {
                "path": {
                    "id": "not-found-tag"
                }
            }
        },
        "patch": {
            "200": {
                "path": {
                    "id": "patch-tag"
                },
                "body": {
                  "body": {
                    "name": "updated-tag-name"
                  }
                }
            },
            "404": {
                "path": {
                    "id": "not-found-tag"
                },
                "body": {
                  "body": {
                    "name": "updated-tag-name"
                  }
                }
            }
        }
    }
}
